{
    "name": "deep-in-fe",
    "version": "1.0.0",
    "description": "Store my code to explore the javascript mystery",
    "private": true,
    "main": "index.js",
    "type": "commonjs",
    "engines": {
        "node": "12.18.0"
    },
    "author": "YuTengjing",
    "license": "MIT",
    "scripts": {
        "test": "mocha",
        "coverage": "nyc --reporter=lcov yarn test",
        "test-report": "mocha --reporter mochawesome",
        "lint": "eslint -c .eslintrc.js --ext .js {src,test}/**/*.js",
        "clean": "rimraf -rf .nyc_output coverage mochawesome-report",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "{src,test}/**/*.js": [
            "eslint -c .eslintrc.js"
        ],
        "*.{js,json,md}": [
            "prettier --write"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@storybook/addon-actions": "^5.3.18",
        "@storybook/addon-links": "^5.3.18",
        "@storybook/addons": "^5.3.18",
        "@storybook/react": "^5.3.18",
        "audit-ci": "^3.0.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "codecov": "^3.7.1",
        "eslint": "^7.0.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-eslint-comments": "^3.1.2",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-mocha": "^6.3.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "husky": "^4.2.5",
        "intelli-espower-loader": "^1.0.1",
        "lint-staged": "^10.2.2",
        "mocha": "^7.1.2",
        "mochawesome": "^6.1.1",
        "node-sass": "^4.14.1",
        "nyc": "^15.0.1",
        "power-assert": "^1.6.1",
        "prettier": "^2.0.5",
        "promises-aplus-tests": "^2.1.2",
        "rimraf": "^3.0.2",
        "sass-loader": "^8.0.2",
        "stylelint": "^13.3.3",
        "stylelint-config-prettier": "^8.0.1",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
        "stylelint-order": "^4.0.0",
        "stylelint-scss": "^3.17.2",
        "webpack-merge": "^4.2.2"
    },
    "dependencies": {}
}
